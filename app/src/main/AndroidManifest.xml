<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.udacity.fatma.musicalappdesign">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MyMusicActivity"
            android:label="My Music " />
        <activity
            android:name=".PaymentActivity"
            android:label="Payment" />
        <activity
            android:name=".PodCastActivity"
            android:label="Podcast" />
        <activity
            android:name=".SongDetailActivity"
            android:label="Now Playing" />
        <activity
            android:name=".SongListActivity"
            android:label="Song List "></activity>
    </application>

</manifest>